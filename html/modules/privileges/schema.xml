<?xml version="1.0" encoding="utf-8"?>
<schema name="xaraya">
  <table name="privileges">
    <column id="privileges.id" name="id" required="true" auto="true">
      <number size="11" decimals="0"/>
    </column>
    <column id="privileges.name" name="name" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="privileges.realm_id" name="realm_id" required="false" auto="false">
      <number size="11" decimals="0"/>
    </column>
    <column id="privileges.module_id" name="module_id" required="false" auto="false">
      <number size="11" decimals="0"/>
    </column>
    <column id="privileges.component" name="component" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="privileges.instance" name="instance" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="privileges.level" name="level" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="privileges.description" name="description" required="true" auto="false">
      <text size="255" defaultvalue=""/>
    </column>
    <column id="privileges.type" name="type" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <constraints>
      <primary name="id">
        <column ref="privileges.id"/>
      </primary>
      <unique name="i_privileges_name">
        <column ref="privileges.name"/>
        <column ref="privileges.module_id"/>
        <column ref="privileges.type"/>
      </unique>
      <index name="i_privileges_realm_id">
        <column ref="privileges.realm_id"/>

      </index>
      <index name="i_privileges_module">
        <column ref="privileges.module_id"/>
      </index>
      <index name="i_privileges_level">
        <column ref="privileges.level"/>
      </index>
    </constraints>
  </table>
  <table name="security_acl">
    <column id="security_acl.partid" name="partid" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="security_acl.permid" name="permid" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <constraints>
      <primary name="partid">
        <column ref="security_acl.partid"/>
        <column ref="security_acl.permid"/>
      </primary>
      <index name="i_security_acl_partid">
        <column ref="security_acl.partid"/>
      </index>
      <index name="i_security_acl_permid">
        <column ref="security_acl.permid"/>
      </index>
    </constraints>
  </table>
  <table name="security_instances">
    <column id="security_instances.id" name="id" required="true" auto="true">
      <number size="11" decimals="0"/>
    </column>
    <column id="security_instances.module_id" name="module_id" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="security_instances.component" name="component" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="security_instances.header" name="header" required="true" auto="false">
      <text size="255" defaultvalue=""/>
    </column>
    <column id="security_instances.query" name="query" required="true" auto="false">
      <text size="255" defaultvalue=""/>
    </column>
    <column id="security_instances.ddlimit" name="ddlimit" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="security_instances.propagate" name="propagate" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="security_instances.instancetable2" name="instancetable2" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="security_instances.instancechildid" name="instancechildid" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="security_instances.instanceparentid" name="instanceparentid" required="true" auto="false">
      <text size="100" defaultvalue=""/>
    </column>
    <column id="security_instances.description" name="description" required="true" auto="false">
      <text size="255" defaultvalue=""/>
    </column>
    <constraints>
      <primary name="id">
        <column ref="security_instances.id"/>
      </primary>
    </constraints>
  </table>
  <table name="security_realms">
    <column id="security_realms.id" name="id" required="true" auto="true">
      <number size="11" decimals="0"/>
    </column>
    <column id="security_realms.name" name="name" required="true" auto="false">
      <text size="255" defaultvalue=""/>
    </column>
    <constraints>
      <primary name="id">
        <column ref="security_realms.id"/>
      </primary>
    </constraints>
  </table>
  <table name="privmembers">
    <column id="privmembers.id" name="id" required="false" auto="false">
      <number size="11" decimals="0"/>
    </column>
    <column id="privmembers.parentid" name="parentid" required="false" auto="false">
      <number size="11" decimals="0"/>
    </column>
    <constraints>
      <index name="i_privmembers_pid">
        <column ref="privmembers.id"/>
      </index>
      <index name="i_privmembers_parentid">
        <column ref="privmembers.parentid"/>
      </index>
    </constraints>
  </table>
</schema>
