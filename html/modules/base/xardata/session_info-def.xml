<?xml version="1.0" encoding="utf-8"?>
<schema name="xaraya">
  <table name="session_info">
    <column id="session_info.id" name="id" required="true" auto="false">
      <text size="32"/>
    </column>
    <column id="session_info.ip_addr" name="ip_addr" required="true" auto="false">
      <text size="20"/>
    </column>
    <column id="session_info.first_use" name="first_use" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="session_info.last_use" name="last_use" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="session_info.role_id" name="role_id" required="true" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <column id="session_info.vars" name="vars" required="false" auto="false">
      <binary/>
    </column>
    <column id="session_info.remember" name="remember" required="false" auto="false">
      <number size="11" defaultvalue="0" decimals="0"/>
    </column>
    <constraints>
      <primary name="id">
        <columnref id="session_info.id"/>
      </primary>
      <index name="i_session_role_id">
        <columnref id="session_info.role_id"/>
      </index>
      <index name="i_session_last_use">
        <columnref id="session_info.last_use"/>
      </index>
    </constraints>
  </table>
</schema>
