<?xml version="1.0"?>
<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
    <form method="post" action="&xar-modurl-dynamicdata-admin-create;">
        <table>
            <xar:data-form module="$moduleid" itemtype="$itemtype" itemid="0" />
            <tr>
                <td class="xar-align-center" colspan="2">
                    <input type="submit" value="#xarML('Create Item')#" />
                </td>
            </tr>
        </table>
        <input type="hidden" name="moduleid" id="moduleid" value="#$moduleid#" />
        <input type="hidden" name="itemtype" id="itemtype" value="#$itemtype#" />
        <input type="hidden" name="itemid" id="itemid" value="0" />
        <input type="hidden" name="authid" id="authid" value="#xarSecGenAuthKey('dynamicdata')#" />
        <input type="hidden" name="return_url" id="return_url"
        value="#xarServer::getCurrentURL()#" />
    </form>
</xar:template>